<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SheetBehavior_Layout">
        <attr name="behavior_sheet_peekSize" format="dimension"/>
        <attr name="behavior_sheet_hideable" format="boolean"/>
        <attr name="behavior_sheet_skipCollapsed" format="boolean"/>
        <attr name="behavior_sheet_leftHanded" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="Ratio_Params">
        <attr name="ratio_widthRatio" format="integer"/>
        <attr name="ratio_heightRatio" format="integer"/>
    </declare-styleable>
    
    <declare-styleable name="MeltingView">
        <attr name="melting_primary_color" format="color"/>
        <attr name="melting_secondary_color" format="color"/>
        <attr name="melting_animation_duration" format="integer"/>
        <attr name="melting_progress" format="float"/>
        <attr name="melting_disable_pre_drawing" format="boolean"/>
    </declare-styleable>
</resources>